{
  "name": "surgereaper-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:design-tokens\"",
    "dev:next": "next dev --turbopack",
    "dev:design-tokens": "chokidar \"src/@design/tokens.json\" -c \"npm run design-tokens\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "design-tokens": "design-tokens -i src/@design/tokens.json -o src/@design/build"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^11.0.0",
    "lucide-react": "^0.471.2",
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.83.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@mogielski/design-tokens": "^1.1.0",
    "@tailwindcss/postcss": "^4.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "postcss": "^8.5.2",
    "tailwindcss": "^4.0.6",
    "typescript": "^5"
  }
}
